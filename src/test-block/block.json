{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "gutenberg-blocks/test-block",
	"version": "0.1.0",
	"title": "Test Block",
	"category": "widgets",
	"icon": "smiley",
	"description": "Example block scaffolded with Create Block tool.",
	"supports": {
		"html": false
	},
	"attributes": {
		"testInput": {
			"type": "string",
			"default": "",
			"componentType": "input",
			"key": "testInput",
			"attributes": {
				"label": "Testing label",
				"type": "number"
			}
		},
		"testToggle": {
			"type": "boolean",
			"default": false,
			"componentType": "toggle",
			"key": "testToggle",
			"attributes": {
				"label": "Testing toggle"
			}
		},
		"testRange": {
			"type": "number",
			"default": 1,
			"componentType": "range",
			"key": "testRange",
			"attributes": {
				"min": 1,
				"max": 300
			}
		}
	},
	"textdomain": "gutenberg-blocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}